!     weakphasearc
!     strongphasearc
!     weakphasesector
!     strongphasesector
!     rematch_arcs_thin
!     get_boundary(posmarke,betx_ip5,bety_ip5)
!     antisymmetrize
!     get_timestamp
!     copytable
!     make_ir5table
!     crossing_save
!     crossing_disable
!     crossing_restore
